{
  "name": "education-api",
  "version": "0.0.1",
  "description": "Service with api for education app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ${npm_package_main}",
    "dev": "nodemon --exec \"NODE_ENV=dev ts-node src/index.ts\"",
    "build": "rm -rf dist/ && tsc --project ./ && copyfiles -u 1 src/**/*.yaml dist/",
    "test": "NODE_ENV=test nyc mocha src/tests/index.ts src/tests/**/*.test.ts --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danil952/education-api.git"
  },
  "keywords": [
    "education"
  ],
  "author": "Danil Garbazhii",
  "license": "ISC",
  "homepage": "https://github.com/danil952/education-api",
  "dependencies": {
    "amqplib": "^0.8.0",
    "aws-sdk": "^2.837.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.0.0",
    "body-parser": "^1.19.0",
    "bson": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^16.5.3",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^8.2.2",
    "mongoose": "^5.11.10",
    "multer": "^1.4.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.16",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/fbemitter": "^2.0.32",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mocha": "^8.2.2",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.1",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "nyc": "^15.1.0",
    "prettier": "^1.18.2",
    "pureimage": "^0.3.2",
    "sinon": "^10.0.0",
    "slugify": "^1.5.0",
    "standard-version": "^8.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "~4.3.0",
    "web3-typescript-typings": "^0.10.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts, js}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
